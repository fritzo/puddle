{
  "name": "puddle",
  "version": "1.0.0",
  "description": "Main puddle repo with documentation and some modules in active development",
  "main": "index.js",
  "scripts": {
    "install": "cd node_modules; ls | xargs -n 1 -P 8 sh -c '(cd $1; npm install)' sh; done",
